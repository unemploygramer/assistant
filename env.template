# Copy this file to .env and fill in values.
# NOTE: In this workspace, .env is intentionally protected/ignored â€” create it manually.

# OpenRouter API Key (for GPT-4o-mini)
OPENROUTER_KEY=

# ElevenLabs API Key (for text-to-speech)
ELEVENLABS_KEY=

# ElevenLabs Voice ID (for main AI - Glitch)
ELEVENLABS_VOICE_ID=

# ElevenLabs Customer Service Voice ID (for phone server)
CUSTOMER_SERVICE_WOMEN=

# Google Calendar
# The calendar id to use (often your Gmail address, or the calendar's ID from Google Calendar settings)
CALENDAR_ID=

# Optional: override where the Service Account JSON lives.
# If unset, server expects ./credentials.json in the project root.
GOOGLE_APPLICATION_CREDENTIALS=credentials.json

# Twilio (for phone server)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=
MY_CELL_NUMBER=

# Phone Server (optional - defaults to 3001)
PHONE_SERVER_PORT=3001

# Public URL for serving audio files (if using ngrok or public domain)
# Example: PUBLIC_URL=https://your-ngrok-url.ngrok.io
PUBLIC_URL=

# N8N Betting Webhook (for NBA player props)
# Your n8n webhook URL that processes betting requests
N8N_BETTING_WEBHOOK_URL=

# Email Notifications (for phone server)
# Gmail address to send/receive lead notifications
BUSINESS_OWNER_EMAIL=
# Gmail App Password (generate at: https://myaccount.google.com/apppasswords)
EMAIL_APP_PASSWORD=

